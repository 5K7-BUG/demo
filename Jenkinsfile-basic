pipeline {
  agent any
  stages {
    stage("Build"){
      steps {
        echo "Building the application"
      }
    }
    stage ("Test") {
      steps {
        echo "Testing the application"
      }
    }
    stage ("package"){
      steps {
        echo" Packinging the application code"
      }
    }
    stage ("Deploy") {
      steps {
        echo "Deploying the application"
      }
    }
  }
}
